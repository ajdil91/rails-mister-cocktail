<h1 class="text-center">Dose</h1>
<div class="container">
  <div class="row">
    <div class="col-xs-12 col-md-6 col-md-offset-3">
      <%= simple_form_for [@cocktail, @dose] do |f| %>
        <%= f.association :ingredient, collection: Ingredient.all %>
        <%= f.input :description %>
        <%= f.submit "Add Ingredient", class: "btn btn-success" %>
      <% end %>
    </div>
  </div>
</div>
